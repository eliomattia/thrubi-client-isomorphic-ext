{
  "name": "thrubi-client-isomorphic",
  "version": "0.1.0",
  "description": "Thrubi",
  "homepage": "/",
  "author": "Elio Mattia",
  "license": "GPL-3.0-or-later",
  "directories": {
    "test": "test"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,scss"
    }
  },
  "scripts": {
    "dev": "nodemon --exec babel-node server-isomorphic/thrubiServerIsomorphic.js",
    "watch": "npm-watch",
    "start": "node server-isomorphic/thrubiServerIsomorphic.js",
    "test": "react-scripts test",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/eliomattia/thrubi-client-isomorphic.git"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.4.1",
    "bootstrap-react": "^0.3.1",
    "d3-array": "^2.4.0",
    "d3-axis": "^1.0.12",
    "d3-format": "^1.4.3",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "ignore-styles": "^5.0.1",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "serialize-javascript": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "web3": "^1.0.0-beta.55"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "nodemon": "^1.18.9",
    "npm-watch": "^0.6.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react/jsx-pascal-case": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
